{"ast":null,"code":"import axios from \"axios\";\nimport swal from \"sweetalert2\";\nexport default {\n  data() {\n    return {\n      \"isLoading\": false\n    };\n  },\n  methods: {\n    doRegister: async function () {\n      const form = event.target;\n      const formData = new FormData(form);\n      this.isLoading = true;\n      const response = await axios.post(this.$apiURL + \"/registration\", formData);\n      this.isLoading = false;\n      swal.fire(\"Registration\", response.data.message, response.data.status);\n      if (response.data.status == \"success\") {\n        form.reset();\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAuCI,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,IAAG,MAAO,aAAY;AAE7B,eAAe;EACXC,IAAI,GAAG;IACH,OAAO;MACH,WAAW,EAAE;IACjB;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,UAAU,EAAE,kBAAkB;MAC1B,MAAMC,IAAG,GAAIC,KAAK,CAACC,MAAK;MACxB,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAACJ,IAAI;MAElC,IAAI,CAACK,SAAQ,GAAI,IAAG;MAEpB,MAAMC,QAAO,GAAI,MAAMX,KAAK,CAACY,IAAI,CAC7B,IAAI,CAACC,OAAM,GAAI,eAAe,EAC9BL,QAAO,CACX;MAEA,IAAI,CAACE,SAAQ,GAAI,KAAI;MACrBT,IAAI,CAACa,IAAI,CAAC,cAAc,EAAEH,QAAQ,CAACT,IAAI,CAACa,OAAO,EAAEJ,QAAQ,CAACT,IAAI,CAACc,MAAM;MAErE,IAAIL,QAAQ,CAACT,IAAI,CAACc,MAAK,IAAK,SAAS,EAAE;QACnCX,IAAI,CAACY,KAAK,EAAC;MACf;IACJ;EACJ;AACJ","names":["axios","swal","data","methods","doRegister","form","event","target","formData","FormData","isLoading","response","post","$apiURL","fire","message","status","reset"],"sourceRoot":"","sources":["/home/aka/Documents/react-nativ/ecommerce-mevn-stack/client/src/components/RegisterComponent.vue"],"sourcesContent":["<template>\n    <div class=\"section no-pad-bot\" id=\"index-banner\">\n        <div class=\"container\" style=\"margin-bottom: 50px;\">\n            <div class=\"row\">\n                <div class=\"col offset-m3 m6\">\n                    <h1 class=\"text-center\" style=\"margin-bottom: 50px; text-align: center;\">Register</h1>\n\n                    <form method=\"POST\" v-on:submit.prevent=\"doRegister\">\n                        <div class=\"form-group\">\n                            <label class=\"text-white\">Enter name</label>\n                            <input type=\"text\" class=\"form-control\" name=\"name\" autocomplete=\"off\" />\n                        </div>\n\n                        <br />\n\n                        <div class=\"form-group\">\n                            <label class=\"text-white\">Enter email</label>\n                            <input type=\"email\" class=\"form-control\" name=\"email\" autocomplete=\"off\" />\n                        </div>\n\n                        <br />\n\n                        <div class=\"form-group\">\n                            <label class=\"text-white\">Enter password</label>\n                            <input type=\"password\" class=\"form-control\" name=\"password\" autocomplete=\"off\" />\n                        </div>\n\n                        <br />\n\n                        <input type=\"submit\" value=\"Register\" v-bind:disabled=\"isLoading\" name=\"submit\" class=\"btn btn-primary\" />\n                    </form>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n\n    import axios from \"axios\"\n    import swal from \"sweetalert2\"\n\n    export default {\n        data() {\n            return {\n                \"isLoading\": false\n            }\n        },\n\n        methods: {\n            doRegister: async function () {\n                const form = event.target\n                const formData = new FormData(form)\n\n                this.isLoading = true\n\n                const response = await axios.post(\n                    this.$apiURL + \"/registration\",\n                    formData\n                )\n                \n                this.isLoading = false\n                swal.fire(\"Registration\", response.data.message, response.data.status)\n\n                if (response.data.status == \"success\") {\n                    form.reset()\n                }\n            }\n        }\n    }\n</script>\n\n<style>\n    label {\n        font-size: initial;\n    }\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}